{
  "cleanupPeriodDays": 30,
  "includeCoAuthoredBy": true,
  "permissions": {
    "deny": [
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(rm:*)",
      "Bash(sudo:*)",
      "Read(**/.env)",
      "Read(**/.env.*)",
      "Read(**/env)",
      "Read(**/environment)",
      "Edit(**/.env)",
      "Edit(**/.env.*)",
      "Edit(**/env)",
      "Edit(**/environment)"
    ],
    "defaultMode": "allowEdits"
  },
  "env": {
    "ANSIBLE_HOST_KEY_CHECKING": "False"
  }
}