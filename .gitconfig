[core]
	excludesfile = /Users/bob/dotfiles/.gitignore
  editor = vim

[hub]
protocol = https
[user]
	name = bob1983
	email = bob.1983.g@gmail.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[credential]
  helper = osxkeychain
[push]
  default = simple
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[alias]
  st = status 
  cm = commit
  fap = fetch --all --prune
  br = branch
  co = checkout
  fp = push --force-with-lease

  unstage = reset -q HEAD --
  discard = checkout --
  uncommit = reset --mixed HEAD~
  amend = commit --amend

[commit]
	template = /Users/bob/.stCommitMsg

